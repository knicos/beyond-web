{
	"compilerOptions": {
	  "outDir": "./dist/",
	  "noImplicitAny": true,
	  "module": "CommonJS",
	  "target": "ES6",
	  "jsx": "react",
	  "allowJs": true,
	  "moduleResolution": "node",
	  "allowSyntheticDefaultImports": true,
      "downlevelIteration": true,
      "esModuleInterop": true,
      "sourceMap": true,
      "resolveJsonModule": true,
      "lib": ["es2015", "dom", "es5", "es6"]
	}
  }