{
  "env": {
      "browser": true,
      "es2021": true,
      "jest/globals": true
  },
  "extends": [
      "plugin:@typescript-eslint/recommended",
      "plugin:@typescript-eslint/eslint-recommended"
  ],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
      "ecmaFeatures": {
          "jsx": true
      },
      "ecmaVersion": 12,
      "sourceType": "module"
  },
  "plugins": [
      "@typescript-eslint",
      "jest"
  ],
  "rules": {
      "semi": 0,
      "import/no-unresolved": 0,
      "class-methods-use-this": 0,
      "@typescript-eslint/no-empty-function": 0
  }
}
